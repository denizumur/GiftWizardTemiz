{
  "openapi": "3.0.1",
  "info": {
    "title": "GiftWizardTemiz.WebApi",
    "version": "1.0"
  },
  "paths": {
    "/api/GiftFinder/suggestions": {
      "post": {
        "tags": [
          "GiftFinder"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GiftProfileDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/GiftProfileDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/GiftProfileDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "GiftProfileDto": {
        "type": "object",
        "properties": {
          "ageRange": {
            "type": "string",
            "nullable": true
          },
          "gender": {
            "type": "integer",
            "format": "int32"
          },
          "interests": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "style": {
            "type": "string",
            "nullable": true
          },
          "occasion": {
            "type": "string",
            "nullable": true
          },
          "relationship": {
            "type": "string",
            "nullable": true
          },
          "priceRange": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}